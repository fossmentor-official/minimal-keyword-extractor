{
    "name": "Keyword Extractor Dev",
    "build": {
      "dockerfile": "Dockerfile",
      "context": ".."
    },
    "forwardPorts": [8501, 8000],
    "postCreateCommand": "pip install -r requirements.txt",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance"
        ]
      }
    },
    "mounts": [
      "source=${localWorkspaceFolder}/data,target=/app/data,type=bind"
    ]
  }